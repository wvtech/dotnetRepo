<div>
    <div><a href=# ui-sref="App.AddUser" data-ng-show="access.Add">Add User</a>   
     </div>
    <div>
        <a href=# ui-sref="App.ManageUserPermissions" data-ng-show="access.Finalize">Manage Permissions for Users</a>
    </div>
    <div class="form-group topGap-25">
        <input name="Users" ng-model="UserName" type="text" placeholder="Search Users By Name" class="form-control input-md" required>
    </div>
    <script type="text/ng-template" id="ManageProfile.html">
    </script>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>
                    <span>First Name</span>
                </th>
                <th>
                    <span>Last Name</span>
                </th>

                <th style="width:100px;">
                    Email Address
                </th>
                <th style="width:162px;">
                    <span>Organization Name</span>
                </th>
                <th>
                    <span>User Role</span>
                </th>
                <th style="width:94px;">
                    Account Create Date
                </th>
                <th>
                    Actions
                </th>
            </tr>
        </thead>
        <tbody>
            <!--Total Record: {{todos.length}}-->

            <tr style="width: 100%" ng-repeat="item in UserList | filter:UserName ">

                <td>{{item.UserFirstName}}</td>
                <td>{{item.UserLastName}}</td>
                <td>{{item.UserEmailId}}</td>
                <td>{{item.OrgName}}</td>
                <td style="width:162px;">{{item.RoleName}}</td>
                <td>{{item.CreatedOn  | date:"MM/dd/yyyy 'at' h:mma"}}</td>

                <td>
                    <span>
                        <a href=# ui-sref="App.ManageProfile({UserId:item.UserId})" data-ng-show="access.Edit">Edit</a>
                        @*<a href="#" ng-click="showdiv()">show</a>*@
                        <a href="#" data-ng-click="DeleteUserInfo(item,$index)" data-ng-show="access.Delete">Delete</a>
                    </span>

                </td>
            </tr>
        </tbody>
    </table>
 
</div>

 